{
  "name": "context-creator",
  "private": false,
  "version": "0.1.18",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:module": "tsup",
    "patch": "npm run build:module && git add . && git commit -m '%npm_config_m%' && npm version patch && npm publish && git push origin main"
  },
  "dependencies": {
    "context-creator": "^0.1.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "ts-standard": "^12.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "main": "dist/context-creator.cjs",
  "types": "dist/context-creator.d.cts",
  "license": "Unlicense",
  "description": "A functional approach to creating React contexts and providers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidystephenson/context-creator.git"
  },
  "author": "David Y. Stephenson",
  "bugs": {
    "url": "https://github.com/davidystephenson/context-creator/issues"
  },
  "homepage": "https://github.com/davidystephenson/context-creator#readme",
  "keywords": [
    "react",
    "state",
    "store",
    "context",
    "context-creator",
    "functional"
  ]
}
